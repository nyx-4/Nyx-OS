#ifndef _QUEUE
#define _QUEUE

#include <stdint.h>
namespace std {
template <class T> class queue {
  private:
    T      Queue[1000];
    size_t start = 0, end = 0;

  public:
    queue() : start(0), end(0) { }
    void   push(T value) { Queue[end++] = value; }
    T      pop() { return Queue[start++]; }
    bool   empty() { return start == end; }
    size_t size() { return end - start; }
    T     &front() { return Queue[start]; }
    T     &back() { return Queue[end]; }
};
} // namespace std

#endif